{
    "commands": [
        {
            "script": "script.js",
            "name": "Configure Handy Menu",
            "handlers": {
                "run": "onSetup"
            },
            "identifier": "com.sergeishere.plugins.handymenu.setup"
        },
        {
            "script": "script.js",
            "name": "Export collections...",
            "handlers": {
                "run": "onExport"
            },
            "identifier": "com.sergeishere.plugins.handymenu.export"
        },
        {
            "script": "script.js",
            "name": "Import collections...",
            "handlers": {
                "run": "onImport"
            },
            "identifier": "com.sergeishere.plugins.handymenu.import"
        },
        {
            "script": "script.js",
            "handlers": {
                "Startup": "onStart"
            }
        }
    ],
    "menu": {
        "title": "Handy Menu",
        "items": [
            "com.sergeishere.plugins.handymenu.setup",
            "-",
            "com.sergeishere.plugins.handymenu.export",
            "com.sergeishere.plugins.handymenu.import"
        ]
    },
    "identifier": "com.sergeishere.plugins.handymenu",
    "version": "6.0",
    "compatibleVersion": "60",
    "description": "Quick access to the plugins and tools you love through a contextual menu.\nCreate collections of plugins or menus, rename them and assign different shortcuts.",
    "homepage": "https://github.com/sergeishere/HandyMenu-SketchPlugin",
    "author": "Sergey Dmitriev",
    "authorEmail": "el.pajaro.sabio@gmail.com",
    "name": "Handy Menu",
    "icon": "icon.png",
    "appcast": "https://raw.githubusercontent.com/sergeishere/HandyMenu-SketchPlugin/master/appcast.xml",
    "disableCocoaScriptPreprocessor": true
}